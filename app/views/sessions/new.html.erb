<div class="container">
  <br>
  <br>
  <br>
  <br>
  <br>
  <div class="jumbotron centered col-md-6 col-md-offset-3">
    <h1>Log In</h1>
    <br>
    <%= form_tag session_path do %>
      <div>
        <%= label_tag :email %><br/>
        <%= text_field_tag :email %>
      </div>
      <div>
        <%= label_tag :password %><br/>
        <%= password_field_tag :password %>
      </div>
      <div><%= submit_tag "Log In" %></div>
    <% end %>
    <br>
    <br>
    <br>
  </div>  
</div>
