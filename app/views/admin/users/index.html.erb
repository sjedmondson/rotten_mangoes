<h2>Users</h2>

<div>
<%= link_to "Create User", new_admin_user_path %>
</div>
<%= paginate @users %> 
</div>
<ul>
  <% @users.each do |user| %>
  <p>(<%= link_to "Update", edit_admin_user_path(user.id) %>) (<%= link_to "Delete", admin_user_path(user.id), method: :delete, data: {confirm: "You sure?"} %>) ID - <%= user.id %> | <%= user.full_name %> | <%= user.email %></p>
  <% end %>
</ul>
<%= link_to "To Movies", movies_path %>